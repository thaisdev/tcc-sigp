@*@model Virtus.Core.Application.ViewModels.FaixaViewModel*@
@{
    ViewBag.Title = "Editar Beneficiário";
}


<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Editar        <small>Beneficiário</small>
        </h1>
        @Html.Partial("_ErrorSummary")
    </section>
    <section class="content">
        <div class="row">
            <div class="col-md-12">
                <div class="box box-info">
                    <div class="box-header with-border">
                        <h3 class="box-title">Dados do  Beneficiado</h3>
                    </div>
                    @using (Html.BeginForm("Edit", "Cliente", FormMethod.Post, new { @class = "form-horizontal" }))
                    {
                        @Html.AntiForgeryToken()

                        @Html.Partial("_formBeneficiarioEdit")
                    }
                </div>

            </div>
        </div>
    </section>
</div>

